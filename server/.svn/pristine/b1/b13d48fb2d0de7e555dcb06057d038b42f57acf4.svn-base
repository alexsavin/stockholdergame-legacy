package com.stockholdergame.server.dto;

import java.util.Collection;

/**
 * @author Alexander Savin
 *         Date: 25.12.12 17.20
 */
public class ServerStatisticsDto {

    private int onlineUsersCount;

    private Collection<String> onlineUsers;

    public int getOnlineUsersCount() {
        return onlineUsersCount;
    }

    public void setOnlineUsersCount(int onlineUsersCount) {
        this.onlineUsersCount = onlineUsersCount;
    }

    public Collection<String> getOnlineUsers() {
        return onlineUsers;
    }

    public void setOnlineUsers(Collection<String> onlineUsers) {
        this.onlineUsers = onlineUsers;
    }
}
