package com.stockholdergame.server.dto.game;

import com.stockholdergame.server.dto.account.UserDto;

/**
 *
 * @author Aliaksandr Savin
 */
public class ScoreDto extends ScoreValuesDto {

    private UserDto user;

    private String gameVariantName;

    private int movesQuantity;

    private int moveOrder;

    public UserDto getUser() {
        return user;
    }

    public void setUser(UserDto user) {
        this.user = user;
    }

    public String getGameVariantName() {
        return gameVariantName;
    }

    public void setGameVariantName(String gameVariantName) {
        this.gameVariantName = gameVariantName;
    }

    public int getMovesQuantity() {
        return movesQuantity;
    }

    public void setMovesQuantity(int movesQuantity) {
        this.movesQuantity = movesQuantity;
    }

    public int getMoveOrder() {
        return moveOrder;
    }

    public void setMoveOrder(int moveOrder) {
        this.moveOrder = moveOrder;
    }
}
