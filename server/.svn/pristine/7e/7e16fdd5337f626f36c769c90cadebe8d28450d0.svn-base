package com.stockholdergame.server.dto.game.variant;

import java.util.Set;

/**
 * @author Alexander Savin
 *         Date: 8.10.2010 23.21.21
 */
public class CardGroupDto {

    private Long id;

    private String groupName;

    private Integer gamerCardQuantity;

    private Set<CardDto> cards;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getGroupName() {
        return groupName;
    }

    public void setGroupName(String groupName) {
        this.groupName = groupName;
    }

    public Integer getGamerCardQuantity() {
        return gamerCardQuantity;
    }

    public void setGamerCardQuantity(Integer gamerCardQuantity) {
        this.gamerCardQuantity = gamerCardQuantity;
    }

    public Set<CardDto> getCards() {
        return cards;
    }

    public void setCards(Set<CardDto> cards) {
        this.cards = cards;
    }
}
