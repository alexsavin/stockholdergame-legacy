package com.stockholdergame.server.dto.game;

import com.stockholdergame.server.dto.account.UserDto;

import java.util.List;

/**
 * @author Alexander Savin
 *         Date: 21.5.11 14.29
 */
public class GameVariantSummary {

    private Long gameVariantId;

    private Integer competitorsQuantity;

    private String gameStatus;

    private Long gamesQuantity;

    private boolean alreadyJoined;

    private String initiationMethod;

    private List<UserDto> playingUsers;

    public Long getGameVariantId() {
        return gameVariantId;
    }

    public void setGameVariantId(Long gameVariantId) {
        this.gameVariantId = gameVariantId;
    }

    public Integer getCompetitorsQuantity() {
        return competitorsQuantity;
    }

    public void setCompetitorsQuantity(Integer competitorsQuantity) {
        this.competitorsQuantity = competitorsQuantity;
    }

    public String getGameStatus() {
        return gameStatus;
    }

    public void setGameStatus(String gameStatus) {
        this.gameStatus = gameStatus;
    }

    public Long getGamesQuantity() {
        return gamesQuantity;
    }

    public void setGamesQuantity(Long gamesQuantity) {
        this.gamesQuantity = gamesQuantity;
    }

    public boolean isAlreadyJoined() {
        return alreadyJoined;
    }

    public void setAlreadyJoined(boolean alreadyJoined) {
        this.alreadyJoined = alreadyJoined;
    }

    public String getInitiationMethod() {
        return initiationMethod;
    }

    public void setInitiationMethod(String initiationMethod) {
        this.initiationMethod = initiationMethod;
    }

    public List<UserDto> getPlayingUsers() {
        return playingUsers;
    }

    public void setPlayingUsers(List<UserDto> playingUsers) {
        this.playingUsers = playingUsers;
    }
}
