package com.stockholdergame.server.dto.game;

import java.util.Set;

/**
 * @author Alexander Savin
 */
public class MoveStepDto {

    private String stepType;

    private Integer cashValue;

    private Long originalStepId;

    private Set<ShareQuantityDto> shareQuantities;

    private Set<SharePriceDto> sharePrices;

    private Set<CompensationDto> compensations;

    public String getStepType() {
        return stepType;
    }

    public void setStepType(String stepType) {
        this.stepType = stepType;
    }

    public Integer getCashValue() {
        return cashValue;
    }

    public void setCashValue(Integer cashValue) {
        this.cashValue = cashValue;
    }

    public Long getOriginalStepId() {
        return originalStepId;
    }

    public void setOriginalStepId(Long originalStepId) {
        this.originalStepId = originalStepId;
    }

    public Set<ShareQuantityDto> getShareQuantities() {
        return shareQuantities;
    }

    public void setShareQuantities(Set<ShareQuantityDto> shareQuantities) {
        this.shareQuantities = shareQuantities;
    }

    public Set<SharePriceDto> getSharePrices() {
        return sharePrices;
    }

    public void setSharePrices(Set<SharePriceDto> sharePrices) {
        this.sharePrices = sharePrices;
    }

    public Set<CompensationDto> getCompensations() {
        return compensations;
    }

    public void setCompensations(Set<CompensationDto> compensations) {
        this.compensations = compensations;
    }
}
