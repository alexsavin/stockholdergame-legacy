package com.stockholdergame.server.dto.game.variant;

/**
 * @author Alexander Savin
 *         Date: 7.10.2010 21.31.41
 */
public class GameShareDto {

    private Long id;

    private Integer initPrice;

    private Integer initQuantity;

    private String color;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Integer getInitPrice() {
        return initPrice;
    }

    public void setInitPrice(Integer initPrice) {
        this.initPrice = initPrice;
    }

    public Integer getInitQuantity() {
        return initQuantity;
    }

    public void setInitQuantity(Integer initQuantity) {
        this.initQuantity = initQuantity;
    }

    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }
}
