package com.stockholdergame.client.ui.components.chart.chartClasses {
    import com.stockholdergame.client.model.dto.game.CompetitorMoveDto;

    import mx.collections.ArrayCollection;

    [Bindable]
    public class GameChartData {

        public function GameChartData(moveNumber:int, moveOrder:int) {
            this.moveNumber = moveNumber;
            this.moveOrder = moveOrder;
        }

        public var moveNumber:int;
        public var moveOrder:int;
        public var competitorItems:ArrayCollection;
        public var sharePriceItems:ArrayCollection;
        public var competitorMove:CompetitorMoveDto;
    }
}
