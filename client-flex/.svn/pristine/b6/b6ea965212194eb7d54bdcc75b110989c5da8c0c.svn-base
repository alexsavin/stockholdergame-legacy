/* -------------------------- old CSS --------------------------- */


.panelWithShadow {
    dropShadowEnabled: true;
    shadowDistance: 10px;
    shadowDirection: right;
    dropShadowColor: #292821;
}

TileList, List {
    paddingLeft: 1px;
    paddingRight: 1px;
    paddingTop: 1px;
    paddingBottom: 1px;
}

.errorPanel, .warnPanel, .infoPanel, .messagePanel {
    borderStyle: solid;
    borderThickness:1px;
    backgroundColor: #FFFEF3;
    cornerRadius:8px;
    /*backgroundAlpha: 0.8;*/
    paddingTop: 5px;
    paddingRight: 10px;
    paddingBottom: 5px;
    paddingLeft: 10px;
    verticalGap: 5px;
    horizontalGap: 5px;
    fontSize: 14px;
    fontWeight: bold;
}

.errorPanel {
    borderColor: red;
    color: #a20808;
}

.warnPanel {
    borderColor: #888000;
    color: #5E5A3A;
}

.infoPanel {
    borderColor: #067b06;
    color: #074b04;
}

.messagePanel {
    borderColor: #D2CFAD;
    color: #726e46;
}

ScrollBar {
    trackColors: #D2CFAD, #FFFEF3;
}

.messagesCounterStyle {
    fontWeight: bold;
    backgroundColor: #ff4500;
    color: white;
}

.attentionLabel {
    fontSize: 18pt;
}

.attentionLabel {
    fontWeight: bold;
}

.attentionLabel {
    color: #a20808;
}



.boxAsCircle {
